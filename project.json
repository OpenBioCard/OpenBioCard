{
  "name": "OpenBioCard",
  "$schema": "./node_modules/nx/schemas/project-schema.json",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["nx build worker"],
        "parallel": false
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["nx serve worker"],
        "parallel": false
      }
    }
  }
}